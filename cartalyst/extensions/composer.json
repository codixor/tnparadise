{
	"name": "cartalyst/extensions",
	"description": "A starter kit for building a modular, extensions-based application.",
	"keywords": ["extensions", "laravel 4"],
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "Cartalyst LLC",
			"email": "help@cartalyst.com"
		},
		{
			"name": "Ben Corlett",
			"email": "ben.corlett@cartalyst.com",
			"role": "Developer"
		},
		{
			"name": "Bruno Gaspar",
			"email": "bruno.gaspar@cartalyst.com",
			"role": "Developer"
		},
		{
			"name": "Dan Syme",
			"email": "dan.syme@cartalyst.com",
			"role": "Creator & Designer"
		}
	],
	"repositories": [
		{
			"type": "composer",
			"url": "http://packages.cartalyst.com"
		}
	],
	"require": {
		"php": ">=5.3.0",
		"cartalyst/dependencies": "~1",
		"illuminate/database": "~4",
		"illuminate/events": "~4",
		"illuminate/filesystem": "~4"
	},
	"require-dev": {
		"illuminate/container": "~4",
		"illuminate/translation": "~4",
		"illuminate/view": "~4",
		"mockery/mockery": "0.7.2"
	},
	"autoload": {
		"classmap": [
			"src/migrations"
		],
		"psr-0": {
			"Cartalyst\\Extensions": "src"
		}
	},
	"extra": {
		"component":"package",
		"branch-alias": {
			"dev-master": "1.0-dev"
		}
	},
	"minimum-stability": "dev"
}
